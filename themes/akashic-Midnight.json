{
  "name": "Akashic Midnight",
  "type": "dark",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#E0E0E0",
    "activityBar.background": "#000000",
    "activityBar.foreground": "#FFFFFF",
    "activityBar.inactiveForeground": "#8A8A8A",
    "activityBar.border": "#1A1A1A",
    "activityBarBadge.background": "#00E0E0",
    "activityBarBadge.foreground": "#000000",
    "sideBar.background": "#0C0C0C",
    "sideBar.border": "#1A1A1A",
    "sideBarSectionHeader.background": "#121212",
    "editorGroupHeader.tabsBackground": "#000000",
    "tab.inactiveBackground": "#000000",
    "tab.activeBackground": "#121212",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveForeground": "#8A8A8A",
    "tab.border": "#1A1A1A",
    "list.hoverBackground": "#1A1A1A",
    "list.activeSelectionBackground": "#FFFFFF20",
    "list.activeSelectionForeground": "#FFFFFF",
    "input.background": "#1A1A1A",
    "input.border": "#333333",
    "button.background": "#E0E0E0",
    "button.foreground": "#000000",
    "button.hoverBackground": "#FFFFFF",
    "panel.background": "#0C0C0C",
    "panel.border": "#1A1A1A",
    "statusBar.background": "#000000",
    "statusBar.foreground": "#A0A0A0",
    "statusBar.border": "#1A1A1A",
    "focusBorder": "#FFFFFF50",
    "selection.background": "#FFFFFF20",
    "terminal.background": "#000000",
    "terminal.foreground": "#E0E0E0",
    "terminal.ansiBrightGreen": "#34D399",
    "terminal.ansiBrightYellow": "#FBBF24"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A737D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FFA7C4"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#00E0E0"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#FFA7C4"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#FFCC00",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#8F8F8F"
      }
    }
  ]
}
